include $libdir ^ "/smlnj-lib/project"
include "../Util/project"
include "../Parser/project"
include "../Rtl/project"
include "../Backend/project"

unit DECALPHA = "decalpha.sig.sml" {Core Rtl MACHINE}
unit DecAlpha = "decalpha.sml" {
	TilWord32 Name Int Core RTL DECALPHA String Rtl Util Char
	List Stats
}
unit DecAlphaUtils = "decalphautils.sml" {
	Core MACHINEUTILS DecAlpha Int Util
}
unit CallConv = "callconv.sml" {
	Core DecAlpha DecAlphaUtils Util CALLCONV
}
unit DIVMULT = "divmult.sig.sml" {DECALPHA Word32}
unit DivMult = "divmult.sml" {
	Core TilWord32 DecAlpha DecAlphaUtils Util TilWord64 DIVMULT
}
interface ToAlpha = "toalpha.int" {
	MACHINEUTILS TRACETABLE BBLOCK DIVMULT TOASM DecAlpha
}
unit ToAlpha : ToAlpha = "toalpha.sml" {
	TilWord32 Core Name Rtl Int Array Pprtl DecAlpha MACHINEUTILS
	TRACETABLE BBLOCK DIVMULT TOASM Util
}
unit TrackStorage = "trackstorage.sml" {
	Core Int ORD_KEY DecAlpha PRINTUTILS MACHINEUTILS TRACKSTORAGE
	SplaySetFn Util
}
interface Linkalpha = "linkalpha.int" {LINKASM}
unit Linkalpha : Linkalpha = "linkalpha.sml" {
	Util Rtl Stats LINKASM Linkrtl DecAlpha DecAlphaUtils
	IfGraph CallConv Bblock Tracetable DivMult ToAlpha PrintUtils
	TrackStorage Color Chaitin RtlToAsm Recursion Tortl
}
